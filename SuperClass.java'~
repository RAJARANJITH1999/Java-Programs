class Box
{
	private int a;
	
	Box(int x)
	{
		a=x;
	}
	
	System.out.println("value of a="+super.a);
}

class Box1 extends Box
{

	int b;
	Box1(int x,int y)
	{
		super(x);
		b=y;
	}
	
	System.out.println("a="+super.a+" b="+b);
}

class SuperClass
{
	public static void main(String args[])
	{
		Box b=new Box(12);
		Box1 b1=new Box1(1,2);
	}

